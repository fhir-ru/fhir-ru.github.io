# - resource -------------------------------------------------------------------

<http://hl7.org/fhir/Endpoint/example-iid> a fhir:Endpoint;
  fhir:nodeRole fhir:treeRoot;
  fhir:Resource.id [ fhir:value "example-iid"];
  fhir:DomainResource.text [
     fhir:Narrative.status [ fhir:value "generated" ];
     fhir:Narrative.div "<div xmlns=\"http://www.w3.org/1999/xhtml\">\n			Example of an Imaging IID type endpoint\n		</div>"
  ];
  fhir:Endpoint.status [ fhir:value "active"];
  fhir:Endpoint.connectionType [
     fhir:Coding.system [ fhir:value "http://terminology.hl7.org/CodeSystem/endpoint-connection-type" ];
     fhir:Coding.code [ fhir:value "ihe-iid" ]
  ];
  fhir:Endpoint.name [ fhir:value "PACS Hospital Invoke Image Display endpoint"];
  fhir:Endpoint.payloadType [
     fhir:index 0;
     fhir:CodeableConcept.text [ fhir:value "DICOM IID" ]
  ];
  fhir:Endpoint.address [ fhir:value "https://pacs.hospital.org/IHEInvokeImageDisplay"] .

