    fhir:Contract.term.securityLabel.classification @<Coding>;  # Confidentiality Protection
    fhir:Contract.term.securityLabel.category @<Coding>*;  # Applicable Policy
    fhir:Contract.term.securityLabel.control @<Coding>*;  # Handling Instructions
